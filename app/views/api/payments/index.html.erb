<p id="notice"><%= notice %></p>

<h1>Api Payments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_payments.each do |api_payment| %>
      <tr>
        <td><%= link_to 'Show', api_payment %></td>
        <td><%= link_to 'Edit', edit_api_payment_path(api_payment) %></td>
        <td><%= link_to 'Destroy', api_payment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api Payment', new_api_payment_path %>
