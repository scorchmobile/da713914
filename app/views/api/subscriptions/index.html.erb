<p id="notice"><%= notice %></p>

<h1>Api Subscriptions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_subscriptions.each do |api_subscription| %>
      <tr>
        <td><%= link_to 'Show', api_subscription %></td>
        <td><%= link_to 'Edit', edit_api_subscription_path(api_subscription) %></td>
        <td><%= link_to 'Destroy', api_subscription, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api Subscription', new_api_subscription_path %>
